{
  "name": "react-starter-template",
  "version": "1.0.0",
  "description": "template for react app",
  "author": "Basile V. <basile64.v@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel public/index.html --open --port 3000",
    "build": "parcel build public/index.html",
    "lint": "eslint src/**/*.js* --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|yml|yaml|css|md|vue)\""
  },
  "keywords": [
    "react"
  ],
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "axios": "^0.21.0",
    "node-sass": "^5.0.0",
    "prop-types": "^15.7.2",
    "public": "^0.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-globe": "^5.0.2",
    "react-icons": "^3.11.0",
    "react-router-dom": "^5.2.0",
    "three": "^0.123.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "sass": "^1.28.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "./node_modules/.bin/eslint --max-warnings=0 --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
